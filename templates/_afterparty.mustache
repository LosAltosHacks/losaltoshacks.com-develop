<div class="afterparty_wrapper">
    <div class="hero">
        <img src="img/bg.png" class="stretch" alt="">
        <div class="overlay">
            <h1 class="desktop-banner">LOS ALTOS HACKS</h1>
            <h1 class="mobile-banner">LOS ALTOS<br>HACKS</h1>
            <a href="#" id="show-arrow" class="arrow"></a>
        </div>
    </div>
    <div class="news">
        <div class="logos">
            <a class="logo" href="https://www.losaltosonline.com/news/sections/schools/210-school-features/52227-">
                <img src="img/latowncrier.png" alt="Los Altos Town Crier" class="darken">
            </a>
            <a class="logo" href="https://channel9.msdn.com/Blogs/raw-tech/Hacking-in-High-School-What-it-takes-to-plan-a-HS-hackathon-with-250-students">
                <img src="img/msdn.png" alt="Microsoft Developer Network" class="darken">
            </a>
            <a class="logo" href="https://medium.com/major-league-hacking/organizing-los-altos-hacks-c0c40ecb1c97">
                <img src="img/medium.png" alt="Medium" class="darken">
            </a>
        </div>
    </div>
    <div id="ap-stats">
        <h1 class="stats-title"><a class="stats-title" href="/2016">LAH 2016</a></h1>
        <h3 class="stats-subtitle">JAN. 30-31</h3>
        <div class="participants">
            <h1 class="hacker-count">176 HACKERS</h1>
            <h1 class="mentor-count">50 MENTORS</h1>
        </div>
        <div class="prizes">
            <h1>$2540 IN PRIZES</h1>
        </div>
        <div class="projects">
            <a href="http://losaltoshacks.devpost.com/submissions">35 HACKS</a>
        </div>
    </div>
    <div class="soon">
        <h4 class="soon-tm-desktop">- LAH 2017 COMING SOON -</h4>
        <h4 class="soon-tm-mobile">LAH 2017<br>- COMING SOON -</h4>
        <div class="email-container">
            <input type="text" placeholder="email" class="email">
            <div class="email-spinner">
                {{> _spinner}}
            </div>
        </div>
        <button class="submit">Get Notified!</button>
    </div>
</div>
